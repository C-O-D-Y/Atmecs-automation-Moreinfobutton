<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Suite" parallel="tests" thread-count="2">
	<test thread-count="1" name="HomepageTest">
		<parameter name="selectBrowser" value="chrome" />
		<classes>
			<class name="com.atmecs.testScripts.HomePagetestscripts" />
		</classes>
	</test>


	<test thread-count="1" name="AnchorsTest">
		<parameter name="selectBrowser" value="ie" />
		<classes>

			<class name="com.atmecs.testScripts.ValidateAnchors" />

		</classes>
	</test>


	<test thread-count="1" name="InsightPageTest">
		<classes>
			<class name="com.atmecs.testScripts.InsightsValidation" />
		</classes>
	</test>

	<test thread-count="1" name="HomepagemoreInfoTest">
		<classes>
			<class
				name="com.atmecs.testScripts.HomePageMoreInfotestscripts" />
		</classes>
	</test>

</suite> <!-- Suite -->
